PS1='\[\033[01;32m\]\u@\h:\[\033[34m\]\w\[\033[31m\]$(__git_ps1 2> /dev/null)\[\033[00m\] # '
export PATH="/opt/software/bin:$PATH"
alias df="df -k"
alias du="du -k"
alias ftp="ftp -i"
alias ls="ls -CF --color=never"
alias quota="quota -v"
if [ -x /usr/bin/vim ]; then
    alias vi="vim -N -n -u NONE"
fi
unalias cp egrep fgrep grep mv p rm which 2> /dev/null
unset command_not_found_handle
