{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "ubuntu-lxc",
        "namespace": "default"
    },
    "spec": {
        "type": "ClusterIP",
        "ports": [
            {
                "name": "ubuntu-web",
                "protocol": "TCP",
                "port": 80,
                "targetPort": 80
            }
        ],
        "selector": {
            "app": "ubuntu-lxc"
        }
    }
}
